@model IEnumerable<RestIT.Controllers.HomeController.GroupByRestaurantCity>
@{
    ViewData["Title"] = "GroupByQuery";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RestName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RestCity)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RestName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RestCity)
            </td>
        </tr>
    }
</table>

